autocmd FileType yaml setlocal et ts=2 ai sw=2 nu sts=2 colorcolumn=1,3,5,7,9,11,80
set dictionary+={{dic_path}}
set complete+=k
set gfn=Liberation\ Mono\ 16

" Sets how many lines of history VIM has to remember
 set history=500

" Enable filetype plugins
filetype plugin on
set ruler
set cmdheight=2
" Ignore case when searching
set ignorecase
set hlsearch
syntax enable 
set encoding=utf8
